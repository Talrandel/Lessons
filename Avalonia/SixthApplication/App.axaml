<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="SixthApplication.App"
             xmlns:local="using:SixthApplication"
             xmlns:themes="clr-namespace:Material.Styles.Themes;assembly=Material.Styles">

    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
  
    <Application.Styles>
      <themes:MaterialTheme PrimaryColor="Purple" BaseTheme="Dark" SecondaryColor="Lime" />
    </Application.Styles>
</Application>